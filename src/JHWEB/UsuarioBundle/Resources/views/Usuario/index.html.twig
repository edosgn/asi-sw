    <h1>Usuario list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Identificacion</th>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>Genero</th>
                <th>Fechanacimiento</th>
                <th>Fijo</th>
                <th>Celular</th>
                <th>Direccion</th>
                <th>Fotografia</th>
                <th>Afiliacion</th>
                <th>Correo</th>
                <th>Fechaexpedicion</th>
                <th>Titulo</th>
                <th>Fechasolicitud</th>
                <th><i class="fa fa-cogs"></i></th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('usuario_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.identificacion }}</td>
                <td>{{ entity.nombres }}</td>
                <td>{{ entity.apellidos }}</td>
                <td>{{ entity.genero }}</td>
                <td>{% if entity.fechaNacimiento %}{{ entity.fechaNacimiento|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.fijo }}</td>
                <td>{{ entity.celular }}</td>
                <td>{{ entity.direccion }}</td>
                <td>{{ entity.fotografia }}</td>
                <td>{{ entity.afiliacion }}</td>
                <td>{{ entity.correo }}</td>
                <td>{% if entity.fechaExpedicion %}{{ entity.fechaExpedicion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.titulo }}</td>
                <td>{% if entity.fechaSolicitud %}{{ entity.fechaSolicitud|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('usuario_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('usuario_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('usuario_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>

